=== tests/cases/conformance/types/negated/negatedTypesControlFlow.ts ===
// broken by https://github.com/Microsoft/TypeScript/pull/29110 (`string & ~("a" | "b")` is non-literal)
// :(

interface A { type: "a", data: number }
>type : "a"
>data : number

interface B { type: "b", data: string }
>type : "b"
>data : string

interface Unknown { type: string & ~("a"|"b"), data: object }
>type : string & ~"a" & ~"b"
>data : object

type ABU = A | B | Unknown;
>ABU : ABU

declare function needsNumber(x: number): void;
>needsNumber : (x: number) => void
>x : number

declare function needsString(x: string): void;
>needsString : (x: string) => void
>x : string

declare function needsObject(x: object): void;
>needsObject : (x: object) => void
>x : object

declare var x: ABU;
>x : ABU

if (x.type === "a") {
>x.type === "a" : boolean
>x.type : "a" | "b" | (string & ~"a" & ~"b")
>x : ABU
>type : "a" | "b" | (string & ~"a" & ~"b")
>"a" : "a"

    let y = x.data;
>y : string | number | object
>x.data : string | number | object
>x : ABU
>data : string | number | object

    needsNumber(y);
>needsNumber(y) : void
>needsNumber : (x: number) => void
>y : string | number | object
}
else if (x.type === "b") {
>x.type === "b" : boolean
>x.type : "b" | (string & ~"a" & ~"b")
>x : ABU
>type : "b" | (string & ~"a" & ~"b")
>"b" : "b"

    needsString(x.data);
>needsString(x.data) : void
>needsString : (x: string) => void
>x.data : string | number | object
>x : ABU
>data : string | number | object
}
else {
    needsObject(x.data);
>needsObject(x.data) : void
>needsObject : (x: object) => void
>x.data : string | number | object
>x : ABU
>data : string | number | object
}

